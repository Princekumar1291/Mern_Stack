<%- include('partials/head') %>
  </head>
  <body class="min-h-screen bg-gray-100 py-6 px-4">
    <div class="max-w-4xl mx-auto">
      <%- include('partials/nav') %>
      <ul class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4">
        <% registeredHomes.forEach(home => { %>
          <li class="bg-white p-4 rounded-lg shadow-md hover:shadow-lg transition-shadow">
            <h2 class="text-xl font-semibold text-gray-800"><%= home.name %></h2>
          </li>
        <% }) %>
      </ul>
    </div>
  </body>
</html>